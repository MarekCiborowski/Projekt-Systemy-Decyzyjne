<Window x:Class="Projekt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Name="MainGrid" Height="450" Width="800" Background="Transparent">
        <Button Content="Load csv file" HorizontalAlignment="Left" Margin="30,10,0,0" VerticalAlignment="Top" Click="LoadFileButton_Click"/>
        <Button Content="Load csv file" HorizontalAlignment="Left" Margin="115,10,0,0" VerticalAlignment="Top" Click="SaveFileButton_Click"/>
        <DataGrid Name="dataGrid" ItemsSource="{Binding DataGridItems}" AutoGenerateColumns="True" Margin="30,74,200,74"/>

    </Grid>
</Window>
